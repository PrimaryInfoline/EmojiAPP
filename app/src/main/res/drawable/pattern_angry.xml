<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="line">
            <stroke android:width="4dp" android:color="#FF0000"/> <!-- Красные линии -->
        </shape>
    </item>
    <item android:top="20dp" android:left="40dp">
        <shape android:shape="line">
            <stroke android:width="6dp" android:color="#990000"/> <!-- Темно-красные линии -->
        </shape>
    </item>
</layer-list>
